<Window x:Class="bulk_image_downloader.MultiLineInput"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:bulk_image_downloader"
        mc:Ignorable="d"
        Title="MultiLineInput" Height="324" Width="628">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="21*"/>
            <ColumnDefinition Width="9*"/>
            <ColumnDefinition Width="116*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="textBox" AcceptsReturn="True" HorizontalAlignment="Left" Height="248" Margin="10,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="600" Grid.ColumnSpan="3"/>
        <Button x:Name="button" Content="OK" Grid.Column="2" HorizontalAlignment="Left" Margin="109,263,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>

    </Grid>
</Window>
